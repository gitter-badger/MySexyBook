{{= it.dots._header(it) }}
<main id="page" class="page-index">
	<div class="container">
		<h1>Bienvenue sur <strong>{{= it.app.title }}</strong> !</h1>

		{{? it.last_users && it.last_users.length }}
		<h2>Derniers inscrits</h2>
		<ul class="users-list" id="last-registered-users">
			{{~ it.last_users :user }}
			<li class="user">
				<a href="{{= it.app.url }}/book/{{= user.pseudo }}">
					<picture class="user-avatar">
						<source srcset="{{= it.app.url }}/avatar/74x74x1/{{= user._id }}" media="(max-width: 32em)" />
						<source srcset="{{= it.app.url }}/avatar/99x99x1/{{= user._id }}" media="(max-width: 40em)" />
						<source srcset="{{= it.app.url }}/avatar/102x102x1/{{= user._id }}" media="(max-width: 50em)" />
						<source srcset="{{= it.app.url }}/avatar/134x134x1/{{= user._id }}" media="(max-width: 60em)" />
						<source srcset="{{= it.app.url }}/avatar/166x166x1/{{= user._id }}" media="(max-width: 80em)" />
						<img srcset="{{= it.app.url }}/avatar/230x230x1/{{= user._id }}" alt="" />
					</picture>
					<span class="user-pseudo">{{= user.pseudo }}</span>
				</a>
			</li>
			{{~}}
		</ul>
		{{?}}

		{{? it.last_photos && it.last_photos.length }}
		<h2>Derni√®res photos mises en ligne</h2>
		<ul class="users-list" id="last-uploaded-photos">
			{{~ it.last_photos :photo }}
			<li class="user">
				<a href="{{= it.app.url }}/book/{{= photo.owner_profile.pseudo }}/{{= photo.album }}#photo-{{= photo._id }}">
					<picture class="user-avatar">
						<source srcset="{{= it.app.url }}/photo/{{= photo.owner }}/{{= photo.album }}/74x74x1/{{= photo.src }}" media="(max-width: 32em)" />
						<source srcset="{{= it.app.url }}/photo/{{= photo.owner }}/{{= photo.album }}/99x99x1/{{= photo.src }}" media="(max-width: 40em)" />
						<source srcset="{{= it.app.url }}/photo/{{= photo.owner }}/{{= photo.album }}/102x102x1/{{= photo.src }}" media="(max-width: 50em)" />
						<source srcset="{{= it.app.url }}/photo/{{= photo.owner }}/{{= photo.album }}/134x134x1/{{= photo.src }}" media="(max-width: 60em)" />
						<source srcset="{{= it.app.url }}/photo/{{= photo.owner }}/{{= photo.album }}/166x166x1/{{= photo.src }}" media="(max-width: 80em)" />
						<img srcset="{{= it.app.url }}/photo/{{= photo.owner }}/{{= photo.album }}/230x230x1/{{= photo.src }}" alt="" />
					</picture>
					{{? photo.owner_profile }}
					<span class="user-pseudo">Par {{= photo.owner_profile.pseudo }}</span>
					{{?}}
				</a>
			</li>
			{{~}}
		</ul>
		{{?}}

		{{= it.dots.search_form(it) }}
	</div>
</main>
{{= it.dots._footer(it) }}